@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Users";
}

<h1>Zarządzanie użytkownikami</h1>
<p>&nbsp;</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                Nazwa użytkownika <!-- UserName -->
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
        </tr>
    </thead>
    <tbody>
            @foreach (var item in Model) {
                    <tr>
                        <td>
                            @item.Id
                        </td>
                        <td>
                            @item.UserName
                        </td>
                        <td>
                            @item.Email
                        </td>
                    </tr>
            }
    </tbody>
</table>
